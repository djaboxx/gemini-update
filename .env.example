# Gemini API key from Google AI Studio (https://ai.google.dev/)
GEMINI_API_KEY=your_api_key_here

# Gemini model to use (default: gemini-1.5-pro)
GEMINI_MODEL=gemini-1.5-pro

# Output directory for generated files
GEMINI_UPDATE_OUTPUT_DIR=./output

# Whether to use Gemini Files API for code analysis (true/false)
GEMINI_UPDATE_USE_FILES=false

# Maximum file size in MB for Gemini Files API (default: 4.0)
GEMINI_UPDATE_MAX_FILE_SIZE=4.0

# Maximum number of files to analyze (default: 100)
GEMINI_UPDATE_MAX_FILES=100
